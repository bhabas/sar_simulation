<?xml version='1.0'?>
<sdf version='1.7'>
  <model name='Test_Plane2'>
    <static>0</static>
    <link name='plane'>
      <pose>0 0 0.0 0 -0 0</pose>
      <inertial>
        <inertia>
          <ixx>500</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>500</iyy>
          <iyz>0</iyz>
          <izz>1000</izz>
        </inertia>
        <mass>1500</mass>
        <pose>0 0 0 0 -0 0</pose>
      </inertial>
      <self_collide>0</self_collide>
      <enable_wind>0</enable_wind>
      <kinematic>0</kinematic>
      <gravity>0</gravity>
      <visual name='plane_visual'>
        <cast_shadows>0</cast_shadows>
        <pose>0 0 0.0 0 0 0</pose>
        <geometry>
          <box>
            <size>1 1 0.01</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Red</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
          <shader type='pixel'>
            <normal_map>__default__</normal_map>
          </shader>
        </material>
      </visual>
      <visual name='plane_center'>
        <cast_shadows>0</cast_shadows>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.025</radius>
          </sphere>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Blue</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
          <shader type='pixel'>
            <normal_map>__default__</normal_map>
          </shader>
        </material>
      </visual>
      <collision name='plane_collision'>
        <laser_retro>0</laser_retro>
        <max_contacts>10</max_contacts>
        <pose>0 0 0 0 -0 0</pose>
        <geometry>
          <box>
            <size>1 1 0.01</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100000</mu>
              <mu2>100000</mu2>
              <fdir1>0 0 0</fdir1>
              <slip1>0</slip1>
              <slip2>0</slip2>
            </ode>
            <torsional>
              <coefficient>1</coefficient>
              <patch_radius>0</patch_radius>
              <surface_radius>0</surface_radius>
              <use_patch_radius>1</use_patch_radius>
              <ode>
                <slip>0</slip>
              </ode>
            </torsional>
          </friction>
          <bounce>
            <restitution_coefficient>0</restitution_coefficient>
            <threshold>1e+06</threshold>
          </bounce>
          <contact>
            <collide_without_contact>0</collide_without_contact>
            <collide_without_contact_bitmask>1</collide_without_contact_bitmask>
            <collide_bitmask>65535</collide_bitmask>
            <ode>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0.2</soft_erp>
              <kp>1e+13</kp>
              <kd>1</kd>
              <max_vel>0.01</max_vel>
              <min_depth>0</min_depth>
            </ode>
            <bullet>
              <split_impulse>1</split_impulse>
              <split_impulse_penetration_threshold>-0.01</split_impulse_penetration_threshold>
              <soft_cfm>0</soft_cfm>
              <soft_erp>0.2</soft_erp>
              <kp>1e+13</kp>
              <kd>1</kd>
            </bullet>
          </contact>
        </surface>
      </collision>
      <sensor name='plane_contact_sensor' type='contact'>
        <always_on>1</always_on>
        <visualize>1</visualize>
        <contact>
          <collision>plane_collision</collision>
          <topic>__default_topic__</topic>
        </contact>
        <plugin name='contact_plugin' filename='libgazebo_ros_bumper.so'>
          <bumperTopicName>/ENV/BodyContact</bumperTopicName>
          <frameName>plane</frameName>
          <alwaysOn>true</alwaysOn>
          <updateRate>500</updateRate>
        </plugin>
        <pose>0 0 0 0 -0 0</pose>
      </sensor>
    </link>
    <joint name="plane_joint" type="fixed">
      <parent>world</parent>
      <child>plane</child>
      <pose>0 0 0 0 0 0</pose>    
      <physics>
        <ode>
        </ode>
        <provide_feedback>true</provide_feedback>
      </physics>
    </joint>
    <static>0</static>
    <allow_auto_disable>1</allow_auto_disable>

    <plugin name="ForceTorque_Sensor" filename="libgazebo_ros_ft_sensor.so">
      <jointName>plane_joint</jointName>
      <topicName>/ENV/Surface_FT_sensor</topicName>
      <updateRate>500</updateRate>
  </plugin>
  </model>
</sdf>
