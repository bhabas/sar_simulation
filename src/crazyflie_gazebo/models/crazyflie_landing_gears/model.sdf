    <plugin name='mavlink' filename='libgazebo_mavlink.so'>
      <robotNamespace/>
      <linkName>crazyflie_body</linkName>
      <mavlink_addr>INADDR_ANY</mavlink_addr>
      <mavlink_port>18080</mavlink_port>
      <motorSpeedCommandPubTopic>/gazebo/command/motor_speed</motorSpeedCommandPubTopic>
      <stickyEnablePubTopic>/gazebo/command/sticky_enable</stickyEnablePubTopic>
    </plugin>

    <plugin name='front_left_motor_model' filename='libgazebo_motor_model.so'>
      <robotNamespace/>
      <jointName>rotor_1_joint</jointName>
      <linkName>rotor_1</linkName>
      <turningDirection>cw</turningDirection>
      <timeConstantUp>0.0001</timeConstantUp>
      <timeConstantDown>0.0001</timeConstantDown>
      <maxRotVelocity>8000</maxRotVelocity> <!-- rad/s - Cap truly implemented in controller because it acts weird here. -->
      <motorConstant>2.21e-8</motorConstant> <!-- This is K_f N/(rad/s)^2-->
      <momentConstant>0.00612</momentConstant> <!-- This is C_tf Nm/N-->
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>0</motorNumber>
      <rotorDragCoefficient>9.1785e-07</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-07</rollingMomentCoefficient>
      <motorSpeedPubTopic>/motor_speed/0</motorSpeedPubTopic> <!-- Motor speed topic for motor #1 -->
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim> <!-- Visual slowdown factor -->
